{
  "id": null,
  "title": "Auto-scaling Framework Dashboard",
  "tags": ["auto-scaling", "monitoring", "performance"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "CPU Utilization",
      "type": "gauge",
      "targets": [
        {
          "expr": "app_cpu_usage_percent",
          "legendFormat": "CPU %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 90}
            ]
          },
          "unit": "percent"
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Memory Utilization",
      "type": "gauge",
      "targets": [
        {
          "expr": "app_memory_usage_bytes / 1024 / 1024 / 1024",
          "legendFormat": "Memory GB"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "min": 0,
          "max": 4,
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 2},
              {"color": "red", "value": 3}
            ]
          },
          "unit": "bytes"
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
    },
    {
      "id": 3,
      "title": "Response Time (95th percentile)",
      "type": "stat",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(app_response_time_seconds_bucket[5m]))",
          "legendFormat": "Response Time"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 1},
              {"color": "red", "value": 2}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
    },
    {
      "id": 4,
      "title": "Request Throughput",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(app_requests_per_second[5m])",
          "legendFormat": "Requests/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": {
            "steps": [
              {"color": "red", "value": null},
              {"color": "yellow", "value": 50},
              {"color": "green", "value": 100}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
    },
    {
      "id": 5,
      "title": "CPU Usage Over Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "app_cpu_usage_percent",
          "legendFormat": "CPU %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 70},
              {"color": "red", "value": 90}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "id": 6,
      "title": "Memory Usage Over Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "app_memory_usage_bytes / 1024 / 1024 / 1024",
          "legendFormat": "Memory GB"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 2},
              {"color": "red", "value": 3}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    },
    {
      "id": 7,
      "title": "Response Time Distribution",
      "type": "heatmap",
      "targets": [
        {
          "expr": "rate(app_response_time_seconds_bucket[5m])",
          "legendFormat": "{{le}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
    },
    {
      "id": 8,
      "title": "Error Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(app_errors_total[5m])",
          "legendFormat": "Errors/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 0.1},
              {"color": "red", "value": 0.3}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
    },
    {
      "id": 9,
      "title": "HTTP Request Duration",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_request_duration_seconds_sum[5m]) / rate(http_request_duration_seconds_count[5m])",
          "legendFormat": "Avg Duration"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
    },
    {
      "id": 10,
      "title": "Resource Utilization by Type",
      "type": "timeseries",
      "targets": [
        {
          "expr": "app_resource_utilization{resource_type=\"cpu\"}",
          "legendFormat": "CPU"
        },
        {
          "expr": "app_resource_utilization{resource_type=\"memory\"}",
          "legendFormat": "Memory"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
    },
    {
      "id": 11,
      "title": "Active Alerts",
      "type": "table",
      "targets": [
        {
          "expr": "ALERTS",
          "legendFormat": "{{alertname}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          }
        }
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "15s",
  "schemaVersion": 30,
  "version": 1
}